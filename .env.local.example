# Password protection for this POC app
WORKLOG_APP_PASSWORD=replace_with_strong_password
WORKLOG_SESSION_SECRET=replace_with_long_random_secret

# Jira Cloud credentials
# Example base URL: https://your-domain.atlassian.net
JIRA_BASE_URL=
JIRA_EMAIL=
JIRA_API_TOKEN=

# Supabase (database-backed mode)
# Use service role key only on server (never expose in browser)
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=

# Optional Jira query / limits
# Default: worklogDate >= startOfMonth(-2)
JIRA_JQL=
# Default: 100
JIRA_MAX_ISSUES=100
